{
	"name": "discordjs-helper",
	"displayName": "Discord.js Helper",
	"description": "Helper for Discord bots development with Discord.js",
	"version": "1.0.2",
	"publisher": "Snowflake107",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"discord",
		"vscode",
		"discord.js",
		"discordbot",
		"discord-snippet"
	],
	"activationEvents": [
		"onCommand:discordjs-helper.docs"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "discordjs-helper.docs",
				"title": "Discord.js Documentation"
			}
		]
	},
	"repository": {
		"url": "git+https://github.com/DevSnowflake/vscode-discord.js-helper.git",
		"type": "git"
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "yarn run test-compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"format": "prettier --write */**.{ts,json,js}"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"prettier": "^2.2.1",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"axios": "^0.21.1"
	},
	"icon": "assets/icon.png",
	"galleryBanner": {
		"color": "#7289da",
		"theme": "dark"
	}
}
